{
    "name": "demo/backend-products",
    "type": "project",
    "description": "API de Produtos - Demo Kubernetes",
    "keywords": ["laravel", "api", "kubernetes"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
